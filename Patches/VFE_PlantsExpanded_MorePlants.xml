<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Plants Expanded - More Plants</li>
    </mods>	
	<match Class="PatchOperationSequence">
      <operations>
        
	      <!--========== ADD CHITIN TO INSECTOIDS DROPS ==========-->
        
	<li Class="PatchOperationAdd">
         <xpath>/Defs/ThingDef[defName = "Plant_SaguaroCactus"]/plant</xpath>
         <value>
 			     <sowTags>
				     <li>VCE_Sandy</li>
			     </sowTags>
         </value>		 
        </li>
        
        
        <li Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName = "Plant_SaguaroCactus"]/plant/fertilityMin</xpath>
         <value>
           <fertilityMin>0</fertilityMin>
         </value>		 
        </li>
        
      </operations>	  
    </match>		  
  </Operation>
</Patch>
