<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Puffin -->
  <ThingDef ParentName="EggUnfertBase">
    <defName>ISA_EggPuffinUnfertilized</defName>
    <label>puffin egg (unfert.)</label>
    <description>An unfertilized puffin egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <color>(228,185,135)</color>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <Nutrition>0.5</Nutrition>
      <MarketValue>9.0</MarketValue>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="EggFertBase">
    <defName>ISA_EggPuffinFertilized</defName>
    <label>puffin egg (fert.)</label>
    <description>A fertilized puffin egg. If all goes well, it should hatch into a baby puffin. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(245,245,245)</color>
      <drawSize>1.2</drawSize>
    </graphicData>
    <statBases>
      <Nutrition>0.5</Nutrition>
      <MarketValue>11.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>0.5</hatcherDaystoHatch>
        <hatcherPawn>ISA_Puffin</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
<!-- Woolly Rhino -->
<ThingDef ParentName="WoolBase">
    <defName>ISA_WoolRhino</defName>
    <label>rhino wool</label>
    <description>rhino's wool. It provides good insulation.</description>
    <statBases>
      <StuffPower_Insulation_Cold>35</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>15</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <color>(89,56,39)</color>
      <commonality>0.15</commonality>
    </stuffProps>
    <graphicData>
      <color>(89,56,39)</color>
    </graphicData>
  </ThingDef>
</Defs>
